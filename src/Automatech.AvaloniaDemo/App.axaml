<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="Automatech.AvaloniaDemo.App"
             xmlns:local="using:Automatech.AvaloniaDemo">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>
  
    <Application.Styles>
        <!-- <FluentTheme /> -->
        <StyleInclude Source="avares://Automatech.Avalonia.Theme/CustomTheme.axaml"></StyleInclude>
    </Application.Styles>
</Application>