<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:u="https://irihi.tech/ursa"
             mc:Ignorable="d" d:DesignWidth="120" d:DesignHeight="450"
             xmlns:vm="clr-namespace:Automatech.AvaloniaApp.ViewModels"
             xmlns:models="clr-namespace:Automatech.AvaloniaApp.Models"
             x:DataType="vm:LeftMenuViewModel"
             x:Class="Automatech.AvaloniaApp.Views.LeftMenuView">
   <u:NavMenu ItemsSource="{Binding Nodes}" CommandBinding="{Binding Command}"
      HeaderBinding="{Binding Header}"
      SubMenuBinding="{Binding Children}" SelectedItem="{Binding }"/>
</UserControl>
